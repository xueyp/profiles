{
  "code block": {
    "prefix": "code",
    "body": ["```$1\n```"],
    "description": "create code block"
  },
  "< symbol": {
    "prefix": "<",
    "body": ["&lt;"],
    "description": "<"
  },
  "> symbol": {
    "prefix": ">",
    "body": ["&gt;"],
    "description": ">"
  },
  "| symbol": {
    "prefix": "|",
    "body": ["&#124;"],
    "description": "&#124;"
  },
  "vim table": {
    "prefix": "vimtable",
    "body": [
      "| 模式 | 键位 | 说明 | 插件 |",
      "| ---- | ---- | ---- | ---- |",
      "|      |      |      |      |"
    ],
    "description": "create vim table"
  },
  "table row": {
    "prefix": "row",
    "body": ["|  $1  |  $2  |  $3  |  $4  |"],
    "description": "create table row"
  },
  "place holder": {
    "prefix": "holder",
    "body": ["<++>"],
    "description": "create placeholder"
  }
}
